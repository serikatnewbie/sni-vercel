import rawContests from "@/data/ctftime/ctf_events.json";

{
  // Process the raw contests
  const contests = rawContests.map((c) => {
    const yearMatch = c.event.match(/\b(20\d{2})\b/);
    const year = yearMatch ? parseInt(yearMatch[1]) : 0;
    
    return {
      place: c.place,
      event: c.event,
      href: c.href,
      ratingPoints: Number(c.ratingPoints),
      ctfPoints: c.ctfPoints,
      year: year,
    };
  });
  
  contests.sort((a, b) => (b.year || 0) - (a.year || 0));
  
  const champions = contests.filter(contest => contest.place === "1");
  
  const notablePlacements = contests.filter(contest => {
    const place = parseInt(contest.place);
    return place > 1 && place <= 10;
  });
  
  function getMedalEmoji(place) {
    switch (place) {
      case "1": return "🥇";
      case "2": return "🥈";
      case "3": return "🥉";
      case "4": return "🏅";
      default:  return "";
    }
  }
  
  function getCrownEmoji(place) {
    return place === "1" ? "👑" : "";
  }
}

## 🥇 Champions 👑
{
  rawContests
    .filter(contest => contest.place === "1")
    .sort((a, b) => {
      const yearA = a.event.match(/\b(20\d{2})\b/);
      const yearB = b.event.match(/\b(20\d{2})\b/);
      return (yearB ? parseInt(yearB[1]) : 0) - (yearA ? parseInt(yearA[1]) : 0);
    })
    .map(contest => {
      const yearMatch = contest.event.match(/\b(20\d{2})\b/);
      const year = yearMatch ? parseInt(yearMatch[1]) : 0;
      let medalEmoji = "";
      if (contest.place === "1") medalEmoji = "🥇";
      const crownEmoji = contest.place === "1" ? "👑" : "";
      
      return (
        <div>
          {medalEmoji} {contest.event} ({year}) {crownEmoji}
        </div>
      );
    })
}

## 📈 Notable High Placements
{
  rawContests
    .filter(contest => {
      const place = parseInt(contest.place);
      return place > 1 && place <= 10;
    })
    .sort((a, b) => {
      const yearA = a.event.match(/\b(20\d{2})\b/);
      const yearB = b.event.match(/\b(20\d{2})\b/);
      return (yearB ? parseInt(yearB[1]) : 0) - (yearA ? parseInt(yearA[1]) : 0);
    })
    .map(contest => {
      const yearMatch = contest.event.match(/\b(20\d{2})\b/);
      const year = yearMatch ? parseInt(yearMatch[1]) : 0;
      const place = parseInt(contest.place);
      let medalEmoji = "";
      
      if (place === 2) medalEmoji = "🥈";
      else if (place === 3) medalEmoji = "🥉";
      else if (place === 4) medalEmoji = "🏅";
      
      const placeSuffix = place > 4 ? ` - ${place}${place === 9 || place === 10 ? "th" : ""} Place` : "";
      
      return (
        <div>
          {medalEmoji} {contest.event} ({year}){placeSuffix}
        </div>
      );
    })
}
